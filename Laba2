	/**
	* $param args
	*/
public static void main(String []args){ 
System.out.println("Hello world!");

		int a = 2;
		int b = 98;
		int c = 78;

		double res  = (((double)(a+ b + c)/3)  + 1)/(a+b);

		System.out.println (res);
	}
}








5 урок 
Main.java
public class Main (

public static void main(String []args){ 

		int a = 2;
		int b = 3;
		int res  =  ((a + b++ - --a) * (++b – a--)) + a + b – a-- + ++a;

		System.out.println (“res: ” +  res);
	}
}
