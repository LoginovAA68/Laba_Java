		}

		*/
int sum = 0;
int count = 0;
int i = -100;

while(i < 200){
if(i  %  4) ==  0){
sum  += il;
count++;
}
i++;
	}

double evg  = (double)sum / count;
System.out.println (“evg:  ” +  evg);
}
}












